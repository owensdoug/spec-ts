{
  "name": "@owensdoug/spec-ts",
  "version": "0.0.1",
  "scripts": {
    "beautify": "prettier --write ./src/*.ts",
    "lint": "eslint --ext ./src/*.ts",
    "lint-fix": "eslint --ext ./src/*.ts --fix",
    "build": "tsc",
    "test": "",
    "version" : "npm run beautify && git add -A src",
    "postversion" : "git push && git push --tags",
    "aws-codeartifact-login": "aws codeartifact login --tool npm --repository spec-ts --domain owensdoug",
    "login": "npm aws-codeartifact-login",
    "publish": "npm publish",
    "bump" : "npm version patch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  }
}
